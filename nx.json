{
  "npmScope": "client",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "admin": {
      "tags": ["admin", "angular"]
    },
    "admin-e2e": {
      "tags": ["admin", "cypress", "e2e"],
      "implicitDependencies": ["admin"]
    },
    "client": {
      "tags": ["client", "angular", "frontend"]
    },
    "e2e": {
      "tags": ["client", "cypress", "e2e"],
      "implicitDependencies": ["client"]
    },
    "server": {
      "tags": ["server", "nest", "backend"]
    },
    "types": {
      "tags": ["types", "lib"]
    },
    "ui": {
      "tags": ["ui", "angular", "lib"]
    }
  }
}
